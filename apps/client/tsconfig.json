{
	"$schema": "https://json.schemastore.org/tsconfig",
	"extends": ["../../tsconfig.json", "./.svelte-kit/tsconfig.json"],
	"compilerOptions": {
		"baseUrl": ".",
		"moduleResolution": "Bundler",
		"paths": {
			"$lib": ["src/lib"],
			"$lib/*": ["src/lib/*"],
		},
		"tsBuildInfoFile": "./node_modules/.cache/tsbuildinfo",
	},
	"include": [
		"../../types/*.d.ts",
		".svelte-kit/",
		"src/",
		"svelte.config.js",
		"vite.config.ts"
	],
}
